<!DOCTYPE html>
<html$if(lang)$ lang="$lang$"$endif$>
<head>
	<meta charset="utf-8">
	<meta name="generator" content="pandoc">
	<meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes">
$for(author-meta)$
	<meta name="author" content="$author-meta$">
$endfor$
$if(date-meta)$
	<meta name="dcterms.date" content="$date-meta$">
$endif$
	<title>$if(title-prefix)$$title-prefix$ - $endif$$pagetitle$$if(title-suffix)$ - $title-suffix$$endif$$if(breadcrumb)$ - $breadcrumb$$endif$</title>
	<style type="text/css">code{white-space: pre;}</style>
	<!--[if lt IE 9]>
		<script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
	<![endif]-->
$if(highlighting-css)$
	<style type="text/css">
$highlighting-css$
	</style>
$endif$
$for(css)$
	<link rel="stylesheet" href="$css$">
$endfor$
$if(math)$
	$math$
$endif$
$for(header-includes)$
	$header-includes$
$endfor$
$if(web)$
	<script type="text/javascript">
		var _gaq = _gaq || [];
		_gaq.push(['_setAccount', 'UA-34854617-1']);
		_gaq.push(['_trackPageview']);
		(function() {
			var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
			ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
			var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
		})();
	</script>
	<script type="text/javascript" src="//use.typekit.net/tsu5tna.js"></script>
	<script type="text/javascript">try{Typekit.load();}catch(e){}</script>
$endif$
</head>
<body>
<header>
<div class="content">
<p class="title">$if(breadcrumb)$<a href="../index.html">$breadcrumb$</a> / $endif$<a href="index.html">$if(title-suffix)$$title-suffix$$else$Feathers Help$endif$</a></p>
$if(search-url)$
<form class="search" action="https://google.com/search" method="get">
	<input type="search" id="query" name="q">
	<input type="hidden" name="sitesearch" value="$search-url$">
	<input type="submit" value="Search">
</form>
$endif$
</div>
</header>
$for(include-before)$
$include-before$
$endfor$
$if(toc)$
<nav id="$idprefix$TOC">
$toc$
</nav>
$endif$
<article>
$body$
</article>
$for(include-after)$
$include-after$
$endfor$
<footer>
<div class="content">
<p>Copyright 2012-2015 Josh Tynjala. All rights reserved.</p>
</div>
</footer>
</body>
</html>
